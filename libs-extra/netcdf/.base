#%Module 1.0 -*- tcl -*-

source [file dirname $::ModulesCurrentModulefile]/../../common/setup.tcl 
GeneralAppSetup

# Additional conflicts
#conflict name

# Prereqs
#prereq gcc/4.9.1 
module load compilers-extra
module load gcc/4.9.1
module load openmpi/1.8.3/gcc-4.9.1

# Specific setup goes here, license files, path setup, etc
setenv          NETCDF          $app_dir
prepend-path    PATH            $app_dir/bin
prepend-path    LD_LIBRARY_PATH $app_dir/lib
prepend-path    LIBRARY_PATH    $app_dir/lib
prepend-path    CPATH           $app_dir/include
prepend-path    MANPATH         $app_dir/share/man 
prepend-path    INFOPATH        $app_dir/share/info

