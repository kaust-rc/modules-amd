#%Module 1.0 -*- tcl -*-

source $env(KAUST_MODULES_ROOT)/common/setup.tcl
GeneralAppSetup
if ![is-loaded gcc/4.7.4] {
    if ![is-loaded compilers-extra] {
        module add compilers-extra
    }
    module add gcc/4.7.4
}
prereq gcc/4.7.4
if ![is-loaded acml/6.0.6] {
    module add acml/6.0.6
}
prereq acml/6.0.6
prepend-path PATH $app_dir
