#%Module 1.0 -*- tcl -*-

source $env(KAUST_MODULES_ROOT)/common/setup.tcl

GeneralAppSetup
AddDepsBasedOnCompiler
AddDeps "python/2.7.10/gcc-4.9.1"

prepend-path PATH $app_dir/bin
